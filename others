Action: file_editor str_replace /app/frontend/src/App.js --old-str "  // Solve the cube
  const solveCube = async () => {
    try {
      setLoading(true);
      const response = await axios.post(`${API}/cube/solve`, cubeState);
      setSolution(response.data);
      setCurrentStep(0);
    } catch (error) {
